                        <DataGrid x:Name="dgridview" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="56,35,115,19" Visibility="Collapsed" Width="NaN" Grid.Column="2"/>

                        <!-- Manufacturer Selection -->
                        <StackPanel Grid.Row="0" Orientation="Horizontal" Margin="10">
                            <Label Content="Select Manufacturer" FontSize="16" VerticalAlignment="Center" />
                            <ComboBox x:Name="Manu_combobox" 
                          Width="200" 
                          FontSize="16" 
                          Margin="10,0,0,0" 
                          IsEditable="False"
                          SelectionChanged="Manu_combobox_SelectionChanged"/>

                        </StackPanel>

                        <!-- DataGrid for Model Selection -->
                        <DataGrid Grid.Row="1" 
                      x:Name="dgridviewDBExplorer" 
                      Margin="10" 
                      IsReadOnly="True" 
                      SelectionChanged="DataGrid_SelectionChanged" HorizontalAlignment="Center"/>

                        <!-- Details Section -->
                        <Border Grid.Row="2" BorderThickness="1" Margin="10" Padding="10" Background="Lavender">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>

                                <!-- Model Field -->
                                <Label x:Name="lblModel" Content="Model" Grid.Column="0" VerticalAlignment="Center" FontSize="14"/>
                                <TextBox x:Name="txtModel" Grid.Column="1"
                             Text="Select model from table above" 
                             FontSize="14" 
                             Margin="10,10,0,0" 
                             IsEnabled="False"
                             TextChanged="txtModel_TextChanged"/>

                                <!-- Type Field -->
                                <Label Content="Type" Grid.Column="2" VerticalAlignment="Center" FontSize="14"/>
                                <TextBox x:Name="txtType" Grid.Column="3" FontSize="14" Margin="10,10,0,0" IsEnabled="False"/>

                                <!-- MOV Min and Max Fields -->
                                <Label Content="MOV Min" Grid.Column="0" Grid.Row="1" VerticalAlignment="Center" FontSize="14"/>
                                <TextBox x:Name="txtMinMOV" Grid.Column="1" Grid.Row="1" FontSize="14" Margin="10,10,0,0" IsEnabled="False"/>
                                <Label Content="MOV Max" Grid.Column="2" Grid.Row="1" VerticalAlignment="Center" FontSize="14"/>
                                <TextBox x:Name="txtMaxMOV" Grid.Column="3" Grid.Row="1" FontSize="14" Margin="10,10,0,0" IsEnabled="False"/>

                                <!-- GDT Min and Max Fields -->
                                <Label Content="GDT Min" Grid.Column="0" Grid.Row="2" VerticalAlignment="Center" FontSize="14"/>
                                <TextBox x:Name="txtMinGDT" Grid.Column="1" Grid.Row="2" FontSize="14" Margin="10,10,0,0" IsEnabled="False"/>
                                <Label Content="GDT Max" Grid.Column="2" Grid.Row="2" VerticalAlignment="Center" FontSize="14"/>
                                <TextBox x:Name="txtMaxGDT" Grid.Column="3" Grid.Row="2" FontSize="14" Margin="10,10,0,0" IsEnabled="False"/>

                                <!-- Stock Code Field -->
                                <Label Content="Stock Code" Grid.Column="0" Grid.Row="3" VerticalAlignment="Center" FontSize="14"/>
                                <TextBox x:Name="txtStockCode" Grid.Column="1" Grid.Row="3" FontSize="14" Margin="10,10,0,0" IsEnabled="False"/>
                            </Grid>
                        </Border>

                        <!-- Action Buttons at the bottom -->
                        <StackPanel Grid.Row="4" Orientation="Horizontal" HorizontalAlignment="Center" Margin="10">
                            <Button x:Name="btnNew" Content="New Entry" FontSize="14" Margin="0,10,10,10" Click="btnNew_Click"/>
                            <Button x:Name="btnEdit" Content="Edit Details" FontSize="14" Margin="0,10,10,10" Click="btnEdit_Click"/>
                            <Button x:Name="btnCan" Content="Cancel" FontSize="14" Margin="0,10,10,10" Click="btnCan_Click" IsEnabled="False"/>
                            <Button x:Name="btnSave" Content="Save" FontSize="14" Margin="0,10,10,10" Click="btnSave_Click" IsEnabled="False"/>
                            <Button x:Name="btnUpdate" Content="Update" FontSize="14" Margin="0,10,10,10" Click="btnUpdate_Click" IsEnabled="False"/>
                            <Button x:Name="btnDelete" Content="Delete" FontSize="14" Margin="0,10,0,10" Click="btnDelete_Click" IsEnabled="False"/>
                        </StackPanel>
                    </Grid>
                </Grid>
            </TabItem>

        </TabControl>
    </Grid>
</Window>

