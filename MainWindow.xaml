<Window x:Class="LSS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LSS"
        xmlns:abcd="clr-namespace:LSS"
        mc:Ignorable="d"
        Title="Lightning Surge Solutions" WindowState="Maximized" Background="RoyalBlue">
    <Window.Resources>
        <abcd:SignatureConverter x:Key="SignatureConverter" />
        <Style TargetType="{x:Type ComboBoxItem}">
            <Style.Triggers>
                <Trigger Property="Content" Value="Fail">
                    <Setter Property="Foreground" Value="Red"/>
                </Trigger>
            </Style.Triggers>
        </Style>
    </Window.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="9*"/>
            <RowDefinition Height="1.5*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="9*"/>
        </Grid.ColumnDefinitions>


        <!-- SPD Data Grid -->
        <Viewbox Grid.Row="0" Grid.Column="0">
            <Image Source="\LSS-logo-oval.png" Stretch="Uniform" />
        </Viewbox>
        <Grid x:Name="liveGrid" Height="340" Grid.Row="1" Grid.Column="0" Width="150" VerticalAlignment="Top" ShowGridLines="True" Margin="0,0,0,0">

            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="30"></ColumnDefinition>
                <ColumnDefinition Width="30"></ColumnDefinition>
                <ColumnDefinition Width="30"></ColumnDefinition>
                <ColumnDefinition Width="30"></ColumnDefinition>
                <ColumnDefinition Width="30"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="40"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
                <RowDefinition Height="30"></RowDefinition>
            </Grid.RowDefinitions>
            <Border BorderThickness="1" BorderBrush="White" Grid.ColumnSpan="5" Grid.RowSpan="11"/>
            <Label Grid.Column="0" Grid.Row="0" Content="Item" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="1" Grid.Row="0" Content="min&#x0a;MOV" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="2" Grid.Row="0" Content="max&#x0a;MOV" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="3" Grid.Row="0" Content="min&#x0a;GDT" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="4" Grid.Row="0" Content="max&#x0a;GDT" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="0" Grid.Row="1" Content="1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="0" Grid.Row="2" Content="2" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="0" Grid.Row="3" Content="3" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="0" Grid.Row="4" Content="4" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="0" Grid.Row="5" Content="5" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="0" Grid.Row="6" Content="6" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="0" Grid.Row="7" Content="7" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="0" Grid.Row="8" Content="8" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="0" Grid.Row="9" Content="9" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>
            <Label Grid.Column="0" Grid.Row="10" Content="10" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="10" FontFamily="Calibri" FontWeight="Bold" Foreground="White"/>

            <TextBlock x:Name="minmov01" Grid.Column="1" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxmov01" Grid.Column="2" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="mingdt01" Grid.Column="3" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxgdt01" Grid.Column="4" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>

            <TextBlock x:Name="minmov02" Grid.Column="1" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxmov02" Grid.Column="2" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="mingdt02" Grid.Column="3" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxgdt02" Grid.Column="4" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>

            <TextBlock x:Name="minmov03" Grid.Column="1" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxmov03" Grid.Column="2" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="mingdt03" Grid.Column="3" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxgdt03" Grid.Column="4" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>

            <TextBlock x:Name="minmov04" Grid.Column="1" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxmov04" Grid.Column="2" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="mingdt04" Grid.Column="3" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxgdt04" Grid.Column="4" Grid.Row="4" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>

            <TextBlock x:Name="minmov05" Grid.Column="1" Grid.Row="5" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxmov05" Grid.Column="2" Grid.Row="5" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="mingdt05" Grid.Column="3" Grid.Row="5" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxgdt05" Grid.Column="4" Grid.Row="5" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>

            <TextBlock x:Name="minmov06" Grid.Column="1" Grid.Row="6" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxmov06" Grid.Column="2" Grid.Row="6" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="mingdt06" Grid.Column="3" Grid.Row="6" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxgdt06" Grid.Column="4" Grid.Row="6" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>

            <TextBlock x:Name="minmov07" Grid.Column="1" Grid.Row="7" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxmov07" Grid.Column="2" Grid.Row="7" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="mingdt07" Grid.Column="3" Grid.Row="7" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxgdt07" Grid.Column="4" Grid.Row="7" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>

            <TextBlock x:Name="minmov08" Grid.Column="1" Grid.Row="8" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxmov08" Grid.Column="2" Grid.Row="8" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="mingdt08" Grid.Column="3" Grid.Row="8" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxgdt08" Grid.Column="4" Grid.Row="8" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>

            <TextBlock x:Name="minmov09" Grid.Column="1" Grid.Row="9" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxmov09" Grid.Column="2" Grid.Row="9" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="mingdt09" Grid.Column="3" Grid.Row="9" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxgdt09" Grid.Column="4" Grid.Row="9" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>

            <TextBlock x:Name="minmov10" Grid.Column="1" Grid.Row="10" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxmov10" Grid.Column="2" Grid.Row="10" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="mingdt10" Grid.Column="3" Grid.Row="10" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
            <TextBlock x:Name="maxgdt10" Grid.Column="4" Grid.Row="10" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White"/>
        </Grid>
        <Button x:Name="btnCreatePDF" Grid.Row="2" Content="Create PDF&#x0a;&amp;&#x0a;Save Data" Width="100" Height="100" FontWeight="Bold" FontSize="14" VerticalAlignment="Bottom" Click="btnCreatePDF_Click"
        HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
        <Button x:Name="btnLoad" Grid.Row="3" Content="Load&#x0a;Certificate" HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="100" Height="70" FontWeight="Bold" FontSize="14" Click="btnLoad_Click" Margin="0,5,0,0"/>
        <Button x:Name="updateBtn" Grid.Row="4" Content="Check for updates" HorizontalAlignment="Center" VerticalAlignment="Bottom" Click="updateBtn_Click" Margin="0,5,0,0" />
        <Label x:Name="versionLbl" Grid.Row="5" Content="Version: " Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Bottom" FontSize="10" Foreground="White"/>
        <Grid x:Name="CoDataBar" Grid.Column="1" HorizontalAlignment="Stretch">
            <Rectangle Height="20" Fill="#FFFF1200" VerticalAlignment="Top" />
            <TextBlock Text="No Company Data Saved" HorizontalAlignment="Center" FontWeight="Bold" />
        </Grid>
        <TabControl x:Name="tabControl" Grid.Row="0" Grid.RowSpan="10"  Grid.Column="1" Grid.ColumnSpan="9" Background="LightGray">
            <!-- Span across all columns -->
            <TabItem Header="Test Certificate Page 1" x:Name="tabPage1">
                <!-- Tab 1 content -->
                <!--<abcd:Tab1Content/>-->
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="9*"/>
                    </Grid.ColumnDefinitions>
                    <Button x:Name="infoButton" HorizontalAlignment="Center" VerticalAlignment="Center" Click="infoButton_Click">
                        <Button.Content>
                            <StackPanel Orientation="Vertical">
                                <Image Source="information-icon.png" Width="50" Height="50" HorizontalAlignment="Center" VerticalAlignment="Center" />
                                <TextBlock Text="Code Guide" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            </StackPanel>
                        </Button.Content>
                    </Button>
                    <ScrollViewer  Grid.Column="1" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                        <Viewbox x:Name="viewBoxPage1" Margin="0,0,20,0" MouseDown="Viewbox_MouseDown" MouseMove="Viewbox_MouseMove" MouseUp="Viewbox_MouseUp">
                            <Viewbox.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform x:Name="scaleTransformP1"/>
                                    <TranslateTransform x:Name="translateTransformP1"/>
                                </TransformGroup>
                            </Viewbox.RenderTransform>
                            <Border Width="1122.52" Height="793.8" Background="White" BorderBrush="Black" BorderThickness="0.2">
                                <Border.Effect>
                                    <DropShadowEffect Color="Black" Direction="315" ShadowDepth="5" Opacity="0.5"/>
                                </Border.Effect>
                                <Canvas Background="White">
                                    <Label Content="BS7671 / BSEN 62305" Canvas.Left="500" Canvas.Top="10" FontSize="9"/>
                                    <Label Content="Surge Protection Certificate" Canvas.Left="418" Canvas.Top="33" FontSize="22"/>

                                    <!--Header-->
                                    <Image x:Name="companyLogoP1" Height="50" Canvas.Left="50" Canvas.Top="17" Width="150" HorizontalAlignment="Left" Stretch="Fill"/>


                                    <TextBlock x:Name="coAddressL1P1" Width="150" Canvas.Right="45" Canvas.Top="17" FontSize="10" TextAlignment="Right"/>
                                    <TextBlock x:Name="coAddressL2P1" Width="150" Canvas.Right="45" Canvas.Top="28" FontSize="10" TextAlignment="Right"/>
                                    <TextBlock x:Name="coAddressL3P1" Width="150" Canvas.Right="45" Canvas.Top="39" FontSize="10" TextAlignment="Right"/>
                                    <TextBlock x:Name="coAddressL4P1" Width="150" Canvas.Right="45" Canvas.Top="50" FontSize="10" TextAlignment="Right"/>
                                    <TextBlock x:Name="coTelNumberP1" Width="150" Canvas.Right="45" Canvas.Top="61" FontSize="10" TextAlignment="Right"/>


                                    <!--Primary Job Details Grid-->
                                    <Grid Height="60" Canvas.Left="50" Canvas.Top="80" Width="1030" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <Grid.Resources>
                                            <SolidColorBrush x:Key="brush" Color="Black"/>
                                        </Grid.Resources>

                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="8*" />
                                            <ColumnDefinition Width="8*" />
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="2*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                        </Grid.RowDefinitions>
                                        <Border Grid.Column="0" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="#FFE7E6E6">
                                            <TextBlock Text="Reference " FontSize="12" FontFamily="Calibri" HorizontalAlignment="Right" VerticalAlignment="Center" FontWeight="Bold"/>
                                        </Border>
                                        <Border Grid.Column="1" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                            <TextBox x:Name="txtBoxReference" FontFamily="Calibri" FontSize="10" VerticalAlignment="Center" BorderThickness="0,0,0,0" TextChanged="txtBoxReference_TextChanged"/>
                                        </Border>
                                        <Border Grid.Column="2" Grid.ColumnSpan="3" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="#FFE7E6E6">
                                            <TextBlock Text="Certificate Number " FontFamily="Calibri" FontSize="12" HorizontalAlignment="Right" VerticalAlignment="Center" FontWeight="Bold"/>
                                        </Border>
                                        <Border Grid.Column="5" Grid.ColumnSpan="5" Grid.Row="0" BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0" Height="20" VerticalAlignment="Top">
                                            <TextBox x:Name="txtBoxCertNum" FontFamily="Calibri" FontSize="10" VerticalAlignment="Center" BorderThickness="0,0,0,0" TextChanged="txtBoxCertNum_TextChanged"/>
                                        </Border>
                                        <Border Grid.Column="0" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="#FFE7E6E6">
                                            <TextBlock Text="Site / Address " FontFamily="Calibri" FontSize="12" HorizontalAlignment="Right" VerticalAlignment="Center" FontWeight="Bold"/>
                                        </Border>
                                        <Border Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="9" BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                            <TextBox x:Name="txtSiteAddress" FontFamily="Calibri" FontSize="10" VerticalAlignment="Center" BorderThickness="0,0,0,0" TextChanged="txtSiteAddress_TextChanged"/>
                                        </Border>
                                        <Border Grid.Column="0" Grid.Row="2" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 1" Background="#FFE7E6E6">
                                            <TextBlock Text="Engineer / Inspector " FontFamily="Calibri" FontSize="12" HorizontalAlignment="Right" VerticalAlignment="Center" FontWeight="Bold"/>
                                        </Border>
                                        <Border Grid.Column="1" Grid.Row="2" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 1">
                                            <ComboBox x:Name="comboEngineer"  FontFamily="Calibri" FontSize="10" BorderThickness="0,0,0,0" Height="19" VerticalAlignment="Top" DropDownClosed="comboEngineer_DropDownClosed"/>
                                        </Border>
                                        <Border Grid.Column="2" Grid.ColumnSpan="2" Grid.Row="2" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 1" Background="#FFE7E6E6">
                                            <TextBlock Text="Date of inspection " FontFamily="Calibri" FontSize="12" HorizontalAlignment="Right" VerticalAlignment="Center" FontWeight="Bold"/>
                                        </Border>
                                        <Border Grid.Column="4" Grid.Row="2" Grid.ColumnSpan="2" BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 1">
                                            <DatePicker x:Name="txtDate" FontFamily="Calibri" FontSize="10" BorderThickness="0,0,0,0" Foreground="Black" VerticalAlignment="Top" FirstDayOfWeek="Monday" SelectedDateFormat="Long" SelectedDateChanged="txtDate_SelectionChanged" Margin="0,0,0,0"/>
                                        </Border>
                                        <Border Grid.Column="6" Grid.ColumnSpan="2" Grid.Row="2" BorderBrush="{StaticResource brush}" BorderThickness="0 1 1 1" Background="#FFE7E6E6">
                                            <TextBlock Text="Next Inspection Due " FontFamily="Calibri" FontSize="12" HorizontalAlignment="Right" VerticalAlignment="Center" FontWeight="Bold"/>
                                        </Border>
                                        <Border Grid.Column="8" Grid.ColumnSpan="2" Grid.Row="2" BorderBrush="{StaticResource brush}" BorderThickness="0 1 1 1">
                                            <TextBlock x:Name="tbDueDate" FontFamily="Calibri" FontSize="10" VerticalAlignment="Center"/>
                                        </Border>
                                    </Grid>

                                    <!--Test Equipment Grid-->
                                    <Grid Height="60" Canvas.Left="50" Canvas.Top="155" Width="1030" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <Grid.Resources>
                                            <SolidColorBrush x:Key="brush" Color="Black"/>
                                        </Grid.Resources>

                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="3*" />
                                            <ColumnDefinition Width="2*" />
                                            <ColumnDefinition Width="3*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                        </Grid.RowDefinitions>
                                        <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="4" BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0" Background="#FFC8C7C7">
                                            <TextBlock Text="Test Equipment" FontSize="12" FontFamily="Calibri" HorizontalAlignment="Center" VerticalAlignment="Center" FontWeight="Bold"/>
                                        </Border>
                                        <Border Grid.Column="0" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="#FFE7E6E6">
                                            <TextBlock Text="MFT / Earth Loop Test Equipment " HorizontalAlignment="Right" VerticalAlignment="Center" FontFamily="Calibri" FontSize="12" FontWeight="Bold"/>
                                        </Border>
                                        <Border Grid.Column="1" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                            <TextBlock x:Name="tbSelEngMFTTester" FontFamily="Calibri" FontSize="10" VerticalAlignment="Center"/>
                                        </Border>
                                        <Border Grid.Column="2" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="#FFE7E6E6">
                                            <TextBlock Text="Serial Number " HorizontalAlignment="Right" VerticalAlignment="Center" FontFamily="Calibri" FontSize="12" FontWeight="Bold"/>
                                        </Border>
                                        <Border Grid.Column="3" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0">
                                            <TextBlock x:Name="tbSelEngMFTSN" FontFamily="Calibri" FontSize="10" VerticalAlignment="Center"/>
                                        </Border>
                                        <Border Grid.Column="0" Grid.Row="2" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 1"  Background="#FFE7E6E6">
                                            <TextBlock Text="SPD Test Equipment " HorizontalAlignment="Right" VerticalAlignment="Center" FontFamily="Calibri" FontSize="12" FontWeight="Bold"/>
                                        </Border>
                                        <Border Grid.Column="1" Grid.Row="2" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 1">
                                            <TextBlock x:Name="tbSelEngSPDTester" FontFamily="Calibri" FontSize="10" VerticalAlignment="Center"/>
                                        </Border>
                                        <Border Grid.Column="2" Grid.Row="2" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 1" Background="#FFE7E6E6">
                                            <TextBlock Text="Serial Number " HorizontalAlignment="Right" VerticalAlignment="Center" FontFamily="Calibri" FontSize="12" FontWeight="Bold"/>
                                        </Border>
                                        <Border Grid.Column="3" Grid.Row="2" BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 1">
                                            <TextBlock x:Name="tbSelEngSPDSN" FontFamily="Calibri" FontSize="10" VerticalAlignment="Center"/>
                                        </Border>
                                    </Grid>
                                    <!--Condition Summary Grid-->
                                    <Grid Height="260" Canvas.Left="50" Canvas.Top="230" Width="1030" HorizontalAlignment="Left" VerticalAlignment="Center">
                                        <Grid.Resources>
                                            <SolidColorBrush x:Key="brush" Color="Black"/>
                                        </Grid.Resources>


                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="1*" />
                                            <ColumnDefinition Width="3*" />
                                            <ColumnDefinition Width="6*" />
                                            <ColumnDefinition Width="6*" />
                                            <ColumnDefinition Width="6*" />
                                            <ColumnDefinition Width="5*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="2*" />
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                            <RowDefinition Height="1*" />
                                        </Grid.RowDefinitions>

                                        <Border Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="6" BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0" Background="#FFC8C7C7">
                                            <TextBlock Text="Condition Summary" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="12" FontFamily="Calibri" FontWeight="Bold"/>
                                        </Border>
                                        <Border Grid.Column="0" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
                                            <TextBlock Background="#FFE7E6E6"/>
                                        </Border>
                                        <Border Grid.Column="1" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="#FFE7E6E6">
                                            <TextBlock Text="Source&#x0a;Protected" TextAlignment="Center" FontSize="12" FontFamily="Calibri" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Border>
                                        <Border Grid.Column="2" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="#FFE7E6E6">
                                            <TextBlock Text="Location" TextAlignment="Center" FontSize="12" FontFamily="Calibri" FontWeight="Bold" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                        </Border>
                                        <Border Grid.Column="3" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="#FFE7E6E6">
                                            <TextBlock Text="Surge Protection Device" TextAlignment="Center" FontSize="12" FontFamily="Calibri" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Border>
                                        <Border Grid.Column="4" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0" Background="#FFE7E6E6">
                                            <TextBlock Text="Comments (See Code Key below)" TextAlignment="Center" FontSize="12" FontFamily="Calibri" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Border>
                                        <Border Grid.Column="5" Grid.Row="1" BorderBrush="{StaticResource brush}" BorderThickness="1 1 1 0" Background="#FFE7E6E6">
                                            <TextBlock Text="Status" TextAlignment="Center" FontSize="12" FontFamily="Calibri" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                        </Border>

                                        <!--Row 1-->
                                        <Border Grid.Column="0" Grid.Row="2" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0"  Background="#FFE7E6E6">
                                            <Label Content="1" HorizontalAlignment="Center" FontSize="9" FontFamily="Calibri" FontWeight="Bold" VerticalAlignment="Center"/>
                                        </Border>
                                        <Border Grid.Column="1" Grid.Row="2" BorderBrush="{StaticResource brush}" BorderThickness="1 1 0 0">
